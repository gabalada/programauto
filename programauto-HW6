#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main(void) {

int mat[3][3];
int i;
int j;
srand(time(NULL));

for (i =0; i<3; i++){
 for (j=0; j<3; j++){
   mat[i][j] = rand() %10;
   }
}

int det = (((mat[0][0]*mat[1][1]*mat[2][2])+(mat[0][1]*mat[1][2]*mat[2][0])+(mat[0][2]*mat[1][0]*mat[2][1]))-((mat[2][0]*mat[1][1]*mat[0][2])+(mat[2][1]*mat[1][2]*mat[0][0])+(mat[2][2]*mat[1][0]*mat[0][1])));
printf("\n o determinante da matriz Ã©: %d\n", det);

return 0
}
